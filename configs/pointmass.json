{
  "algo": "RL2",
  "env": "pointmass",
  "obs_mode": "image",
  "meta_batch_size": 80,
  "hidden_sizes": [
    64
  ],
  "rollouts_per_meta_task": 2,
  "parallel": false,
  "max_path_length": 25,
  "discount": 0.99,
  "gae_lambda": 1.0,
  "normalize_adv": true,
  "positive_adv": false,
  "learning_rate": 1e-3,
  "max_epochs": 5,
  "max_baseline_epochs": 100,
  "baseline_loss_mult": 2.0,
  "double_camera":false,
  "cell_type": "lstm",
  "num_minibatches": 1,
  "n_itr": 1000
}
